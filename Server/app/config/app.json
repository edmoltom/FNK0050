{
  "enable_vision": true,
  "enable_ws": true,
  "enable_movement": true,
  "vision": {
    "interval_sec": 0.1,
    "camera_fps": 15,
    "mode": "face",
    "face": {
      "resize_ratio": 0.5,
      "min_size": [40, 40],
      "scale_factor": 1.1,
      "min_neighbors": 5
    }
  },
  "ws": {
    "host": "0.0.0.0",
    "port": 8765
  },
  "behavior": {
    "social_fsm": {
      "deadband_x": 0.12,
      "lock_frames_needed": 3,
      "miss_release": 5,
      "interact_ms": 1500,
      "relax_timeout": 30
    }
  }
}
