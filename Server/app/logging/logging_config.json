{
  "root": {
    "level": "INFO",
    "file": "robot.log",
    "max_bytes": 1048576,
    "backup_count": 3,
    "format": "%(asctime)s [%(levelname)s] [%(name)s] %(message)s"
  },
  "modules": {
    "core": "WARNING",
    "mind": "DEBUG",
    "interface": "INFO",
    "app": "INFO",
    "sandbox": "WARNING"
  },
  "silenced": [
    "interface.voice_interface",
    "core.movement.gait_cpg"
  ]
}
